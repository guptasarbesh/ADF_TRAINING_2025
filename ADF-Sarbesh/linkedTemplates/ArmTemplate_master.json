{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-Sarbesh"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"LS_DS_ConnectionString_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_DS_ConnectionString'"
		},
		"LS_SQLServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SQLServer'"
		},
		"LS_ADLS_KeyValut_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sarbeshstorage.dfs.core.windows.net/"
		},
		"LS_DS_ConnectionString_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sarbeshstorage.dfs.core.windows.net/"
		},
		"LS_SQLServer_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sarbeshgupta.database.windows.net"
		},
		"LS_SQLServer_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sarbesh_db_sql"
		},
		"LS_SQLServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sarbeshgupta"
		},
		"LSsarbeshkeyValut_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://sarbesh-key-valut.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-Sarbesh_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"LS_DS_ConnectionString_accountKey": {
						"value": "[parameters('LS_DS_ConnectionString_accountKey')]"
					},
					"LS_SQLServer_password": {
						"value": "[parameters('LS_SQLServer_password')]"
					},
					"LS_DS_ConnectionString_properties_typeProperties_url": {
						"value": "[parameters('LS_DS_ConnectionString_properties_typeProperties_url')]"
					},
					"LS_SQLServer_properties_typeProperties_server": {
						"value": "[parameters('LS_SQLServer_properties_typeProperties_server')]"
					},
					"LS_SQLServer_properties_typeProperties_database": {
						"value": "[parameters('LS_SQLServer_properties_typeProperties_database')]"
					},
					"LS_SQLServer_properties_typeProperties_userName": {
						"value": "[parameters('LS_SQLServer_properties_typeProperties_userName')]"
					},
					"LSsarbeshkeyValut_properties_typeProperties_baseUrl": {
						"value": "[parameters('LSsarbeshkeyValut_properties_typeProperties_baseUrl')]"
					},
					"LS_ADLS_KeyValut_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_KeyValut_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADF-Sarbesh_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADF-Sarbesh_ArmTemplate_0')]"
			]
		}
	]
}